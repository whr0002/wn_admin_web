@model IEnumerable<wn_Admin.Models.UtilityModels.LinkViewModel>
@{
    ViewBag.Title = "Dashboard";
}


<div class="row mCustomDiv">
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <div class="panel panel-primary">
                @*<div class="panel-heading"><h4>@item.LinkName</h4></div>*@
                <div class="panel-body"><h3>@item.LinkName</h3></div>
                <div class="panel-footer clearfix">
                    <div class="pull-right">
                        <a href="@item.Link" class="btn btn-primary btn-lg" target="_blank">Go</a>
                    </div>
                </div>
            </div>
        </div>
    }
</div>






